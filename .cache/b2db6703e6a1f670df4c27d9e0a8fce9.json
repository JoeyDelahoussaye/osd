nts/App.jsx","original":{"line":4,"column":18}},{"name":"Component","generated":{"line":150,"column":8},"source":"components/App.jsx","original":{"line":4,"column":24}},{"name":"module","generated":{"line":152,"column":0},"source":"components/App.jsx","original":{"line":90,"column":0}},{"name":"exports","generated":{"line":152,"column":7},"source":"components/App.jsx","original":{"line":90,"column":7}},{"generated":{"line":152,"column":14},"source":"components/App.jsx","original":{"line":90,"column":0}},{"name":"App","generated":{"line":152,"column":17},"source":"components/App.jsx","original":{"line":90,"column":17}},{"generated":{"line":152,"column":20},"source":"components/App.jsx","original":{"line":90,"column":0}}],"sources":{"components/App.jsx":"const React = require('react');\nconst Utilities = require('../utilities');\n\nclass App extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      email: '',\n      password: '',\n    };\n  }\n\n  handleEmailChange(event) {\n    this.setState({ email: event.target.value });\n  }\n\n  handlePasswordChange(event) {\n    this.setState({ password: event.target.value });\n  }\n\n  addUser() {\n    const { email } = this.state;\n    const { password } = this.state;\n    Utilities.addNewUser(email, password);\n  }\n\n  render() {\n    return (\n      <div className=\"container-fluid\">\n        <div className=\"row\">\n          <div className=\"col-md-6\">\n            <h3>\n              Official Sponsors of Democracy\n            </h3>\n            <dl>\n              <dt>\n                  Support Your Employees\n              </dt>\n              <dd>\n                  Bolster employee morale by letting them know that you support their right to vote\n              </dd>\n              <dt>\n                  Logistics Made Easy\n              </dt>\n              <dd>\n                  We create a logistical gameplan custom-tailored to each employee voter\n              </dd>\n              <dt>\n                No Surprises\n              </dt>\n              <dd>\n                Get ahead of election-day scheduling so business goes smoothly\n              </dd>\n            </dl>\n          </div>\n          <div className=\"col-md-6\">\n            <div role=\"form\">\n              <div className=\"form-group\">\n\n                <label htmlFor=\"exampleInputEmail1\">\n                      Email address\n                  <input type=\"email\" className=\"form-control\" id=\"exampleInputEmail1\" email={this.value} onChange={this.handleEmailChange.bind(this)} />\n                </label>\n              </div>\n              <div className=\"form-group\">\n                <label htmlFor=\"exampleInputPassword1\">\n                      Password\n                  <input type=\"password\" className=\"form-control\" id=\"exampleInputPassword1\" password={this.value} onChange={this.handlePasswordChange.bind(this)} />\n                </label>\n              </div>\n              <a href=\"../BusinessFormIndex.html\">\n                <button type=\"submit\" className=\"btn btn-primary\" onClick={this.addUser.bind(this)}>\n                    Submit\n                </button>\n              </a>\n            </div>\n            <div>\n                      Already a member?\n              <button type=\"button\" id=\"existingMember\" className=\"btn btn-background\">\n                Sign In Here\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nmodule.exports = App;\n"},"lineCount":null}},"hash":"53148be1a6c611cc07e9c81c776ced4e","cacheData":{"env":{}}}